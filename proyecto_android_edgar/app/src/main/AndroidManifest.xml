<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.edgar.proyecto_android_edgar">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icono_apk"
        android:label="Team Manager"

        android:roundIcon="@drawable/icono_apk"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activityEditarJugador"></activity>
        <activity
            android:name=".activityInfo"
            android:label="" />
        <activity
